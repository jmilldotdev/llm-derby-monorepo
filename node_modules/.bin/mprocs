#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jmill/projects/bcad/llm-derby-monorepo/node_modules/.pnpm/mprocs@0.7.2/node_modules/mprocs/node_modules:/Users/jmill/projects/bcad/llm-derby-monorepo/node_modules/.pnpm/mprocs@0.7.2/node_modules:/Users/jmill/projects/bcad/llm-derby-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jmill/projects/bcad/llm-derby-monorepo/node_modules/.pnpm/mprocs@0.7.2/node_modules/mprocs/node_modules:/Users/jmill/projects/bcad/llm-derby-monorepo/node_modules/.pnpm/mprocs@0.7.2/node_modules:/Users/jmill/projects/bcad/llm-derby-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mprocs/cli.js" "$@"
else
  exec node  "$basedir/../mprocs/cli.js" "$@"
fi
